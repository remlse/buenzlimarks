{
    "files.associations": {
      "*.css": "tailwindcss"
    },
    "editor.quickSuggestions": {
      "strings": true
    },
		"eslint.workingDirectories": [
			"./app",
		],
		"eslint.packageManager": "pnpm",
		"editor.codeActionsOnSave": {
			"source.fixAll.eslint": true
		},
    "[markdown]": {
      "editor.formatOnSave": true,
    },
    "prettier.proseWrap": "always",
}